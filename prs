void saad() {
    string s, t; cin >> s >> t;
    vector<string> v;
    stringstream ss(s);
    string i;
    while (getline(ss, i, ',')) v.pb(i);
    int ans = 0;
    function<void(int)> check = [&] (int i) {
        if (i == sz(t)) {
            ans++;
            return;
        }
        string tmp = "";
        while (i < sz(t)) {
            tmp += t[i];
            fli(j, 0, sz(v)) if (tmp == v[j]) check(i + 1);
            i++;
        }
    };
    check(0);
    cout << ans << nl;
}
