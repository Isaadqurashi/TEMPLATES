#include <bits/stdc++.h>
#define fli(fc,n) for(int i=fc;i<n;i++)
#define flj(fc,n) for(int j=fc;j<n;j++)
#define rli(n,rc) for(int i=n;i>rc;i--)
#define sz(a) a.size()
#define ll long long
#define pb push_back
#define all(v) v.begin(), v.end()
#define rall(v) v.rbegin(), v.rend()
#define nl "\n"
using namespace std;

void saad() {}

int main() {
    ios::sync_with_stdio(false), cin.tie(nullptr);
    freopen("in.txt","r",stdin);
    freopen("out.txt","w+",stdout);
    int t;
    cin >> t;
    fli(1, t + 1) {
        cout << "Case #" << i << ": ";
        saad();
    }
}
